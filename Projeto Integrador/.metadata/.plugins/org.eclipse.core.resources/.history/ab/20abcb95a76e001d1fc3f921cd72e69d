package classe;

public class Fornecedor extends Pessoa{
	
	public Fornecedor() {
		super();

	}


	public Fornecedor(String nome, String cpf, String dataNascimento, String endereco, String setor, String funcao,
			double salario, String nome2) {
		super(nome, cpf, dataNascimento, endereco, setor, funcao, salario);
		nome = nome2;
	}


	@Override
	public String toString() {
		return "Fornecedor [getNome()=" + getNome() + ", getCpf()=" + getCpf() + ", getDataNascimento()="
				+ getDataNascimento() + ", getEndereco()=" + getEndereco() + ", toString()=" + super.toString()
				+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
	}
	
	
}
