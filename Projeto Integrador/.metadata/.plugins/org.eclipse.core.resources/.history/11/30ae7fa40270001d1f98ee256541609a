package classe;

public class Fornecedor extends Pessoa{
	
	
	public Fornecedor() {
		super();
	}

	public Fornecedor(String nome, String cpf, String dataNascimento, String rua, String numero, String bairro,
			String cep, String cidade, String completemento) {
		super(nome, cpf, dataNascimento, rua, numero, bairro, cep, cidade, completemento);
	}

	@Override
	public String toString() {
		
		return "getNome()" + getNome();
	}
	
}
