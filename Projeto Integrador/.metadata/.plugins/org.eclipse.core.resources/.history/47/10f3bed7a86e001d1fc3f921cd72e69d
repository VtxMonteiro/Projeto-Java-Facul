package classe;

public class Cliente extends Pessoa{

	private double credito;

	public Cliente(){
		
	}
	
	public Cliente(String nome, String cpf, String dataNascimento, String endereco, String setor, String funcao,
			double salario, double credito) {
		this.credito = credito;
	}

	public double getCredito() {
		return credito;
	}

	public void setCredito(double credito) {
		this.credito = credito;
	}

	public Cliente(String nome, String cpf, String dataNascimento, String endereco, String setor, String funcao,
			double salario) {
		super(nome, cpf, dataNascimento, endereco, setor, funcao, salario);
	}

	@Override
	public String toString() {
		return "Cliente [credito=" + credito + ", getCredito()=" + getCredito() + ", getNome()=" + getNome()
				+ ", getCpf()=" + getCpf() + ", getDataNascimento()=" + getDataNascimento() + ", getEndereco()="
				+ getEndereco() + ", toString()=" + super.toString() + ", getClass()=" + getClass() + ", hashCode()="
				+ hashCode() + "]";
	}
	
	
}
