package classe;



public class Prestador extends Pessoa {

	private String servico;
	
	
	public Prestador() {
		super();
	}


	public Prestador(String nome, String cpf, String dataNascimento, String rua, String numero, String bairro,
			String cep, String cidade, String completemento, String servico) {
		super(nome, cpf, dataNascimento, rua, numero, bairro, cep, cidade, completemento);
		this.servico = servico;
	}


	public String getServico() {
		return servico;
	}


	public void setServico(String servico) {
		this.servico = servico;
	}


	@Override
	public String toString() {
		return "Prestador servico=" + servico + "]";
	}
	
}
