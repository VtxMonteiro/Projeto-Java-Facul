package classe;

public class Fornecedor extends Pessoa{

	public Fornecedor() {
		super();

	}

	public Fornecedor(String nome, String marca, String validade, String fabricante, double valor, Fornecedor fornecedor) {
		super(nome, fabricante, nome, marca, validade, fabricante, valor);
		
	}

	@Override
	public String toString() {
		return "Fornecedor [getNome()=" + getNome() + ", getCpf()=" + getCpf() + ", getDataNascimento()="
				+ getDataNascimento() + ", getEndereco()=" + getEndereco() + ", toString()=" + super.toString()
				+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
	}
	
	
}
