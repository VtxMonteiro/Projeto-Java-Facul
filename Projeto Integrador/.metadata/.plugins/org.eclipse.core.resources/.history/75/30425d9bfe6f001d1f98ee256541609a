package classe;



public class Prestador extends Pessoa {

	private String servico;
	
	
	public Prestador() {
		super();
		
	}

	
	public Prestador(String nome, String cpf, String dataNascimento, String endereco, String rua, String bairro,
			String cep, String cidade, String completemento, String servico) {
		super(nome, cpf, dataNascimento, endereco, rua, bairro, cep, cidade, completemento);
		this.servico = servico;
	}


	public String getServico() {
		return servico;
	}

	public void setServico(String servico) {
		this.servico = servico;
	}

	@Override
	public String toString() {
		return "Prestador [toString()=" + super.toString() + ", getNome()=" + getNome() + ", getCpf()=" + getCpf()
				+ ", getDataNascimento()=" + getDataNascimento() + ", getEndereco()=" + getEndereco() + ", getClass()="
				+ getClass() + ", hashCode()=" + hashCode() + "]";
	}
}
